    "description": "20i distributed lock server code. This is deployed to aws",
    "main": "src/main.ts",
    "author": "wolf",
    "license": "ISC",
    "private": true,
    "scripts": {
        "build": "npx tsc",
        "buildclient": "cd srcClient && npm run build",
